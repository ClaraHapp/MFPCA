    foreach,
    irlba,
    Matrix,
    methods,
    mgcv (>= 1.8-33),
    plyr,
    stats
Depends:
    R (>= 3.2.0),
    funData (>= 1.3-4)
Suggests:
    covr,
    fda,
    testthat (>= 2.0.0)
NeedsCompilation: yes
SystemRequirements: libfftw3 (>= 3.3.4)
